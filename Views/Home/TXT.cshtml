
@{
    ViewBag.Title = "TXT";
}


<style>
    .grid-container {
        display: grid;
        align-content: space-evenly;
        grid-template-columns: auto auto auto;
        gap: 2px;
    }

        .grid-container > div {
            background-color: whitesmoke;
            text-align: center;
            padding: 25px 10px;
            font-size: 15px;
        }

    .card {
        box-shadow: 0 4px 8px 0 rgba(0,0,0,0.2);
        transition: 0.3s;
    }

        .card:hover {
            box-shadow: 0 8px 16px 0 rgba(0,0,0,0.2);
        }

    .container {
        padding: 4px 16px;
    }

    .flip-card {
        background-color: transparent;
        width: 300px;
        height: 175px;
        border: 0.5px solid #00ff21;
        perspective: 1000px;
    }

    .flip-card-inner {
        position: relative;
        width: 100%;
        height: 100%;
        text-align: center;
        transition: transform 0.8s;
        transform-style: preserve-3d;
    }

    .flip-card:hover .flip-card-inner {
        transform: rotateY(180deg);
    }

    .flip-card-front, .flip-card-back {
        position: absolute;
        width: 100%;
        height: 100%;
        backface-visibility: hidden;
    }

    .flip-card-front {
        background-color: transparent;
        color: transparent;
    }

    .flip-card-back {
        background-color: teal;
        color: white;
        transform: rotateY(180deg);
    }
</style>


